{
  "name": "reactjs-apollo",
  "version": "1.0.0",
  "description": "ReactJS Apollo Client - Integrated with GraphQL, Express, ES6/ES7, JSX, Webpack, Babel, Material Design Lite, and PostCSS",
  "scripts": {
    "start": "npm run lint && npm run test && npm run e2e-test && nodemon --watch server/data/**/*.js --exec \"babel-node server/index.js\"",
    "test": "mocha --require ignore-styles -w client/tests/helpers/browser.js client/tests/*/*.spec.js --recursive -R xunit client/tests/ --reporter-options output=test-reports.xml",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "e2e-test": "./node_modules/protractor/bin/protractor protractor.conf.js",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./client",
    "update": "babel-node server/utils/updateSchema.js",
    "deploy": "npm run lint && npm run test && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "stage": "npm run lint && npm run test && cross-env NODE_ENV=stage webpack --config webpack.config.js",
    "lint": "eslint --ignore-path .gitignore client server; exit 0",
    "lintest": "npm run lint && npm run test",
    "esdoc": "esdoc",
    "docs": "open ./doc/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danjavia/ReatJS-Apollo-Starter-Kit.git"
  },
  "keywords": [
    "react",
    "relay",
    "graphql",
    "express",
    "es6",
    "es7",
    "jsx",
    "webpack",
    "babel",
    "material design lite",
    "postcss",
    "scaffolding",
    "fullstack"
  ],
  "author": "Danny Viasus (Danjavia)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Danjavia/ReatJS-Apollo-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Danjavia/ReatJS-Apollo-Starter-Kit",
  "devDependencies": {
    "babel-plugin-import": "^1.0.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "clean-webpack-plugin": "^0.1.10",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "favicons-webpack-plugin": "0.0.6",
